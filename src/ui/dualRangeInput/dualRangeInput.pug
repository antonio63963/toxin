mixin dualRangeInput(options) 
  - const {min, max, valueMin, valueMax, isShownLabels} = options || {};
  - const max1 = max - 1; 
  .wrapper(class!= attributes.class)
    .range-slider
      if(isShownLabels)
        .range-labels 
          span.range-label.range-label-start= min
          span.range-label.range-label-end= max
      input.range-slider__input(type="range", value=valueMin, max=max1)
      input.range-slider__input(type="range", value=valueMax, max=max)
      .track-wrapper
        .track 
        .range-between 
        .thumb.left 
        .thumb.right

    